<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	
	<!--==============================================
	============ Legion Caesar Apparel ===============
	===============================================-->	

  <ThingDef ParentName="JDSLegionApparelBase">
		<defName>JDS_Legion_Caesar_Apparel</defName>
		<label>Caesar Apparel</label>
		<description>"The undershirt of the tunic appears to be a red sports jersey"</description>  
	<graphicData>
		<texPath>Things/UI/Caesar_Apparel_UI</texPath>
    </graphicData>
	<apparel>
		<wornGraphicPath>Things/Apparel/Caesar_Apparel/Caesar_Apparel</wornGraphicPath>
		<tags>
			<li>CaesarApparel</li>
		</tags>			
    </apparel>
    <statBases>
		<WorkToMake>8500</WorkToMake>
		<MaxHitPoints>200</MaxHitPoints>
		<Mass>2.2</Mass>
		<Flammability>0.1</Flammability>
		<ArmorRating_Sharp>0.33</ArmorRating_Sharp>
		<ArmorRating_Blunt>0.37</ArmorRating_Blunt>
		<ArmorRating_Heat>0.45</ArmorRating_Heat>
		<Insulation_Cold>12</Insulation_Cold>
		<Insulation_Heat>18</Insulation_Heat>
		<EquipDelay>4</EquipDelay>
    </statBases>
    <equippedStatOffsets>
		<SocialImpact>0.35</SocialImpact>
	</equippedStatOffsets>
	<costList>
		<Cloth>100</Cloth>		
	</costList>	
  </ThingDef>

	<!--==============================================
	================= Legate Armor ===================
	===============================================-->	

  <ThingDef ParentName="JDSLegionApparelBase">
		<defName>JDS_Legion_Legate_Armor</defName>
		<label>Legate Armor</label>
		<description>"The armor appears to be a full suit of heavy gauge steel and offers full body protection. It also has a cloak in excellent condition, but the armor itself bears numerous battle scars and the helmet is missing half of the left horn."</description>  
	<graphicData>
		<texPath>Things/UI/Legate_Armor_UI</texPath>
    </graphicData>
	<apparel>
		<wornGraphicPath>Things/Apparel/Legate_Armor/Legate_Armor</wornGraphicPath>
		<tags>
			<li>LegateArmor</li>
		</tags>			
	</apparel>	
    <statBases>
		<WorkToMake>10000</WorkToMake>
		<MaxHitPoints>220</MaxHitPoints>
		<Mass>4.5</Mass>
		<ArmorRating_Sharp>1.3</ArmorRating_Sharp>
		<ArmorRating_Blunt>0.72</ArmorRating_Blunt>
		<ArmorRating_Heat>0.45</ArmorRating_Heat>
		<Insulation_Cold>11</Insulation_Cold>
		<Insulation_Heat>17</Insulation_Heat>
		<EquipDelay>11</EquipDelay>
    </statBases>
    <equippedStatOffsets>
		<SocialImpact>0.45</SocialImpact>
		<MoveSpeed>-0.12</MoveSpeed>
	</equippedStatOffsets>
	<costList>
		<Steel>500</Steel>
		<Cloth>100</Cloth>		
		<ComponentIndustrial>5</ComponentIndustrial>
	</costList>	
  </ThingDef>
  
